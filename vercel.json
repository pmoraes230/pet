{
    "builds": [
      {
        "src": "setup/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "staticfiles/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "setup/wsgi.py"
      }
    ]
  }